.uni-table {
  min-width: 100% !important;
}
.uni-collapse-item__title-text {
  font-size: 30rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 600;
  color: rgba(0, 0, 0, 0.9);
  line-height: 30rpx;
}
.uni-collapse-item__title-arrow {
  border: 14rpx solid;
  border-color: transparent transparent transparent #d8d8d8;
  width: 10rpx !important;
  height: 10rpx !important;
  -webkit-transform-origin: left center;
          transform-origin: left center;
}
.uni-collapse-item__title-arrow-active {
  -webkit-transform: rotate(90deg) !important;
          transform: rotate(90deg) !important;
}
.uni-collapse-item__title-arrow uni-icons {
  display: none;
}
.silder {
  width: 76% !important;
  margin: 0 !important;
  position: relative !important;
  left: 72rpx !important;
}
.silder .uni-slider-handle-wrapper {
  height: 16rpx !important;
  border-radius: 6rpx !important;
}
.silder::before {
  content: '0%';
  position: absolute;
  left: -72rpx;
  top: 50%;
  -webkit-transform: translate(0, -50%);
          transform: translate(0, -50%);
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(0, 0, 0, 0.6);
}
.silder::after {
  content: '100%';
  position: absolute;
  right: -48px;
  top: 50%;
  -webkit-transform: translate(0, -50%);
          transform: translate(0, -50%);
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(0, 0, 0, 0.6);
}
.uni-textarea-compute {
  font-size: 34rpx !important;
  font-weight: 400 !important;
  color: rgba(0, 0, 0, 0.9) !important;
}
.mp-header {
  z-index: 0;
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 344px;
}
.mp-header .logo-wrapper {
  height: 100%;
  padding: 0;
}
.mp-header .logo-wrapper .logo {
  width: 100%;
  height: 100%;
}
.mp-header .logo-wrapper .logo image {
  width: 100%;
  height: 100%;
}

